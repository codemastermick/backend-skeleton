{
  "name": "demo-backend-server",
  "version": "1.0.0",
  "description": "backend server example",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist/*",
    "compile": "tsc",
    "lint": "eslint -c .eslintrc.js --ext .ts src/ --fix",
    "build": "npm-run-all clean lint compile",
    "dev:start": "npm-run-all build start",
    "dev": "nodemon --watch src -e ts --exec npm run dev:start",
    "start": "node .",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "author": "Codemaster Mick",
  "license": "ISC",
  "dependencies": {
    "express": "^4.17.1",
    "http-status-codes": "^2.2.0",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "mongoose": "^6.1.6",
    "morgan": "^1.10.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.3",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/mongoose": "^5.11.97",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "dotenv": "^10.0.0",
    "eslint": "^8.4.1",
    "eslint-plugin-jsdoc": "^37.2.2",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "jest": "^27.4.5",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "supertest": "^6.1.6",
    "ts-jest": "^27.1.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  },
  "_moduleAliases": {
    "@config": "dist/configs",
    "@lib": "dist/lib",
    "@middleware": "dist/middleware",
    "@routes": "dist/routes"
  }
}
