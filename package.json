{
  "name": "demo-backend-server",
  "version": "1.0.0",
  "description": "backend server example",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist logs",
    "clean:dist": "rimraf dist",
    "clean:logs": "rimraf logs",
    "compile": "webpack",
    "compile:watch": "cross-env NODE_ENV=development webpack",
    "lint": "eslint -c .eslintrc.js --ext .ts src/ --fix",
    "build": "npm-run-all clean compile",
    "dev:start": "nodemon --watch src -e ts,js,json --exec npm run start",
    "dev": "npm-run-all lint dev:start",
    "prestart": "npm-run-all clean compile",
    "start": "node .",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "author": "Codemaster Mick",
  "license": "ISC",
  "dependencies": {
    "express": "^4.17.1",
    "http-status-codes": "^2.2.0",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "mongoose": "^6.1.6",
    "morgan": "^1.10.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.3",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/mongoose": "^5.11.97",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.0",
    "@types/supertest": "^2.0.11",
    "@types/webpack-node-externals": "^2.5.3",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "eslint": "^8.4.1",
    "eslint-plugin-jsdoc": "^37.2.2",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "jest": "^27.4.5",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "prettier-eslint": "^13.0.0",
    "rimraf": "^3.0.2",
    "supertest": "^6.1.6",
    "ts-jest": "^27.1.2",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "typescript": "^4.5.4",
    "webpack": "^5.66.0",
    "webpack-cli": "^4.9.1",
    "webpack-node-externals": "^3.0.0"
  },
  "_moduleAliases": {
    "@config": "dist/configs",
    "@lib": "dist/lib",
    "@middleware": "dist/middleware",
    "@routes": "dist/routes"
